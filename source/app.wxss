.rowitem{
 padding:24rpx; 
  border-bottom:2rpx solid #f2f2f2;
}
.rowitem .imgcover{
  height: 120rpx;
  width: 120rpx;
  box-shadow: 2rpx 2rpx 1rpx #888888;
  border-radius: 4rpx;
}
.rowitem .headtitle{
  padding-left:20rpx;
  color:black;
  font-size: 32rpx;
  font-weight: 550;
}
.rowitem .headdescription{
  padding-left:20rpx;
  font-size: 24rpx;
  margin-top: 8rpx;
  color:#999;
}
.rowitem .headdescriptionicon{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.rowitem .headdescriptionicontext{
  width: 100rpx;
  margin-right: 10rpx;
  color:#999;
}

.findno{
  width: 257rpx;
  height: 257rpx;
}

.rowitem .play{
  height: 40rpx;
  width:40rpx;
  background-color:rgba(255, 255, 255, 0.5);
  position: absolute;
  margin-top: -60rpx;
  margin-left: 10rpx;
  border-radius: 20rpx;
}
.rowitem .playicon{
width: 0; 
height: 0; 
border-top: 10rpx solid transparent; 
border-left: 20rpx solid #51ABD8; 
border-bottom: 10rpx solid transparent; 
margin-left: 12rpx;
margin-top: 10rpx;
}

.blockrow{
 padding:15rpx; 
 border-bottom:2rpx solid #f2f2f2;
 background: white;
}
.blockitem{
  width: 220rpx;
  margin-left: 20rpx;
}
.blockitem .imgcover{
  height: 180rpx;
  width: 180rpx;
  box-shadow: 2rpx 2rpx 1rpx #888888;
  border-radius: 4rpx;
}
.blockitem .headtitle{
  color:black;
  font-size: 28rpx;
  font-weight: 550;
}
.blockitem .headdescription{
  font-size: 24rpx;
  margin-top: 8rpx;
  color:#999;
}
.inbottomla{
  text-align: center;
  font-size: 24rpx;
  padding:10rpx;
  margin-bottom: 20rpx;
  color:#999;
}
.navigator-hover{
  background: transparent;
}

.gridrow{
  display: flex;
  flex-wrap: wrap;
  margin-left: 30rpx;
  padding-top: 30rpx;
}
.grid-item{
  width: 330rpx;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
  background: white;
  border-radius: 10rpx;
  box-shadow: 1rpx 1rpx 1rpx #ddd;
}
.grid-cover{
  width: 330rpx;
  height: 206rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.grid-cover image{
  width: 330rpx;
  height: 206rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
}
.grid-time{
  position: absolute;
  margin-top: -40rpx;
  width: 320rpx;
  text-align: right;
  color: white;
  font-size: 22rpx;
}
.grid-title{
  padding:20rpx;
  font-size: 28rpx;
  height: 60rpx;

  overflow:hidden; 
text-overflow:ellipsis;
display:-webkit-box; 
-webkit-box-orient:vertical;
-webkit-line-clamp:2; 
}
.grid-cat{
  padding:20rpx;
  padding-top: 0rpx;
  font-size: 24rpx;
  color:gray;

}


.tabs{
  box-shadow: 0rpx 2rpx 2rpx #f2f2f2;
  background: white;
  margin-bottom: 2rpx;
}
.tab{
  text-align: center;
  font-size: 26rpx;
}
.tab .tabtext{
  padding-top:18rpx;
  padding-bottom:18rpx;
  color: #999;
  font-size: 26rpx;
}
.tab .active{
  border-bottom: 6rpx solid #51ABD8;
  color:#51ABD8;
}

.maintab{
  height: calc( 100vh - 82rpx );
}
.maintab-item{
  height: 100%;
}


.foreshow-item{
  padding-left: 30rpx;
  padding-right: 30rpx;
  margin-top: 30rpx;
}
.foreshow-item image{
  width:690rpx;
  height:517.5rpx;
  border-radius: 6rpx;
}
.foreshow-item .title{
  position: absolute;
  background-color: rgba(255, 255, 255, 0.7);
  color:white;
  width:670rpx;
  padding:10rpx;
  margin-top: -86rpx;
  border-bottom-left-radius: 6rpx;
  border-bottom-right-radius: 6rpx;
  height: 60rpx;
  overflow:hidden; 
text-overflow:ellipsis;
display:-webkit-box; 
-webkit-box-orient:vertical;
-webkit-line-clamp:1; 
}
.foreshow-item .reminder{
  position: absolute;
  right: 30rpx;
  margin-top: 20rpx;
  background-color:#51ABD8;
  color:white !important;
  padding: 15rpx;
  font-size: 28rpx;
}
.foreshow-item .reminder view{
  color:white;
}
.foreshow-item .reminder image{
  width: 35rpx;
  height: 35rpx;
  margin-right: 10rpx;
}

