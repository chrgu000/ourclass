/**app.wxss**/
page{
  background: #f2f2f2;
}
.clear-contact{
  position: absolute;  
  opacity: 0;
}
.clear-contact-debug{ 
  opacity: 1;
  background: orange;
}
.clear-button{ 
  padding: 0rpx;
  font-size: unset;
  text-align: unset;
  line-height: unset;
  background-color: transparent;
  border: 0rpx red solid;
  border-color:red;
}
.clear-button::after {
  border: 0;
} 
.btn-primary-line{
  border-color: #51ABD8;
  color: #51ABD8;
  background: transparent;
}
.btn-primary-line::after {
  border-color: #51ABD8;
} 
view,navigator,input,text,textarea{
  font-size: 32rpx;
  font-weight: 350;
  color: #2c2c2c;
}
.wordbreak{
  word-break: break-all;
}
.flex-row{
